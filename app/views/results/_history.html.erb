<%- model_class = Result -%>

<table style="background: transparent; border: 0;">
  <thead>
    <tr>
      <th>User</th>
      <th>Change Action</th>
      <th>Time Changed</th>
    </tr>
  </thead>
<tbody style="border: 0;">
  <% result.versions.each do |version| %>
  <tr style="background: transparent; border: 0;">
    <td style="border: 0;"><% if version.whodunnit!= 'anonymous' %>
      <%= User.find(version.whodunnit).name %>
      <% else %>
      <%= version.whodunnit %>
      <% end %>
    </td>
    <td style="border: 0;"><%= version.event %></td>
    <td style="border: 0;"><%= version.created_at %></td>
  </tr>
  <% end %>
</tbody>
</table>
